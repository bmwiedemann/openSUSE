		<param name="parent-tag">v2.0.22</param>
		<!-- 2.0.22 tag was made after 2.1.0 on practically the same branch, go figure -->
		<param name="versionformat">2.1.0+2.0.22+git@TAG_OFFSET@</param>
	</service>
	<service name="recompress" mode="disabled">
		<param name="file">*.tar</param>
		<param name="compression">xz</param>
	</service>
