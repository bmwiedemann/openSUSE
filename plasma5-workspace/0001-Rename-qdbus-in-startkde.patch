 # Get a property value from org.freedesktop.locale1
 queryLocale1() {
-    qdbus --system org.freedesktop.locale1 /org/freedesktop/locale1 "$1"
+    qdbus-qt5 --system org.freedesktop.locale1 /org/freedesktop/locale1 "$1"
