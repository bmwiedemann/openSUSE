  <service mode="manual" name="tar_scm">
    <param name="url">https://github.com/casey/just</param>
    <param name="versionformat">@PARENT_TAG@</param>
    <param name="scm">git</param>
	<param name="exclude">.git</param>
	<param name="exclude">.github</param>
	<param name="exclude">www</param>
    <param name="revision">b31be27f2da63684c75e7dad7e89461ebba78944</param>
    <param name="versionrewrite-pattern">v(.*)</param>
    <param name="versionrewrite-replacement">\1</param>
    <param name="changesgenerate">enable</param>
    <param name="changesauthor">socvirnyl.estela@gmail.com</param>
  </service>
  <service mode="manual" name="recompress">
    <param name="file">*.tar</param>
    <param name="compression">zstd</param>
  </service>
  <service mode="manual" name="set_version"/>
  <service name="cargo_vendor" mode="manual">
     <param name="srctar">just-*.tar.zst</param>
  </service>
  <service name="cargo_audit" mode="manual" />
</services>

