 if(FALSE)
 	add_compile_definitions("JAS_NO_SET_SSIZE_T")
-	add_compile_definitions("ssize_t=__INSERT_APPROPRIATE_TYPE_HERE__"")
+	add_compile_definitions("ssize_t=__INSERT_APPROPRIATE_TYPE_HERE__")
 endif()
 
