diff --git a/seq/aseqdump/aseqdump.c b/seq/aseqdump/aseqdump.c
index 790454020259..578e06fbcb22 100644
--- a/seq/aseqdump/aseqdump.c
+++ b/seq/aseqdump/aseqdump.c
@@ -25,7 +25,7 @@
 #include <string.h>
 #include <signal.h>
 #include <getopt.h>
-#include <sys/poll.h>
+#include <poll.h>
