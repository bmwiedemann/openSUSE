+	If.0 {
+		Condition {
+			Type ControlExists
+			Control "name='Line Boost Volume'"
+		}
+		True {
+			EnableSequence [
 				cset "name='Line Boost Volume' 3"
-			}
+			]
 		}
-	]
+	}
 }
 
