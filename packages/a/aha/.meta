<package name="aha" project="openSUSE:Factory">
  <title>ANSI color to HTML converter</title>
  <description>aha (ANSI HTML Adapter) converts ANSI colors to HTML, e.g. if you
want to publish the output of ls --color=yes, git diff, ccal or htop
as static HTML somewhere.

</description>
  <devel project="utilities" package="aha"/>
  <url>https://github.com/theZiz/aha/</url>
</package>
