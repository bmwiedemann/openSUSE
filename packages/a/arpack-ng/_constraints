<?xml version="1.0" encoding="UTF-8"?>
<constraints>
  <!-- Only for x86_64, aarch64, riscv64 where pyarpack is built -->
  <overwrite>
    <conditions>
      <arch>x86_64</arch>
      <arch>aarch64</arch>
      <arch>riscv64</arch>
      <package>arpack-ng:serial</package>
    </conditions>
    <hardware>
      <disk>
        <size unit="G">3</size>
      </disk>
      <memory>
        <size unit="G">9</size>
      </memory>
    </hardware>
  </overwrite>
</constraints>
