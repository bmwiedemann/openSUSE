<?xml version="1.0" encoding="UTF-8"?>
<constraints>
  <!-- Only for x86_64, aarch64, riscv64 where pyarpack is built -->
  <overwrite>
    <conditions>
      <arch>x86_64</arch>
      <arch>aarch64</arch>
      <arch>riscv64</arch>
    </conditions>
    <hardware>
      <disk>
        <size unit="G">3</size>
      </disk>
      <physicalmemory>
        <size unit="G">12</size>
      </physicalmemory>
    </hardware>
  </overwrite>
</constraints>
