<package name="yast2-ycp-ui-bindings" project="openSUSE:Factory">
  <title>YaST2 - YCP Bindings for the YaST2 User Interface Engine</title>
  <description>This package makes the generic YaST2 user interface engine available
for YCP applications (YCP is the scripting language in which most YaST2
modules are written).
</description>
  <devel project="YaST:Head" package="yast2-ycp-ui-bindings"/>
  <build>
    <disable arch="ppc64"/>
  </build>
</package>
