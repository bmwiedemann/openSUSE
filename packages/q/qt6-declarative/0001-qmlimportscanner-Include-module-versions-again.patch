--- a/tools/qmlimportscanner/main.cpp
+++ b/tools/qmlimportscanner/main.cpp
@@ -45,7 +45,7 @@ using FileImportsWithoutDepsCache = QHash<QString, QVariantList>;
 namespace {
 
