#!/bin/bash

distro=$(rpm --eval '%{?sle_version}:%{?is_opensuse}%{!?is_opensuse:0}')
case "${distro}" in
150200:0)
    TAGPREFIX=suse/sles/15.2
    LABELPREFIX=com.suse.kubevirt
    ;;
150300:0)
    TAGPREFIX=suse/sles/15.3
    LABELPREFIX=com.suse.kubevirt
    ;;
*)
    TAGPREFIX=kubevirt
    LABELPREFIX=org.opensuse.kubevirt
    ;;
esac
