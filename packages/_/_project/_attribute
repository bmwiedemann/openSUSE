installcheck-ignore-duplicated-binaries = 
# busybox-misc has a few file conflicts:   /usr/bin/lspci, /usr/bin/mesg, /usr/bin/wall, /usr/bin/resize, /usr/bin/xxd
# libstdc++6-gccX are non-installable, but they are also not really meant to be used (see https://github.com/openSUSE/openSUSE-release-tools/pull/2968)
# Please keep in sync with ports.
installcheck-ignore-conflicts = busybox-misc libstdc++6-gcc7 libstdc++6-pp-gcc7 libstdc++6-pp-gcc7-32bit libstdc++6-gcc12 libstdc++6-pp-gcc12 libstdc++6-pp-gcc12-32bit
