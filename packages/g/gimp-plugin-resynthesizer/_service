    <param name="exclude">.git</param>
    <param name="filename">gimp-plugin-resynthesizer</param>
    <param name="versionformat">%cd</param>
    <param name="changesgenerate">enable</param>
    <param name="versionprefix">2.0.3</param>
  </service>
