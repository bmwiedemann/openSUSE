<?xml version="1.0" encoding="UTF-8"?>
<ivy-module version="2.0" xmlns:e="http://ant.apache.org/ivy/extra">
	<info organisation="com.eed3si9n" module="sbt-assembly" revision="0.14.2" status="release" publication="20160214222133" e:scalaVersion="2.10" e:sbtVersion="0.13">
		<license name="MIT License" url="https://github.com/sbt/sbt-assembly/blob/master/LICENSE"/>
		<description>
		sbt plugin to create a single fat jar
		</description>
