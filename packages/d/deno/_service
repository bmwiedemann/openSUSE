    <param name="method">registry</param>
    <param name="url">https://github.com/denoland/deno</param>
    <param name="revision">v2.4.2</param>
    <param name="versionrewriteregex">^v?(.*)</param>
    <param name="versionrewritepattern">$1</param>
 </service>
