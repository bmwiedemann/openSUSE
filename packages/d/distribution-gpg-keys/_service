<services>
  <service mode="manual" name="obs_scm">
    <param name="url">https://github.com/rpm-software-management/distribution-gpg-keys.git</param>
    <param name="scm">git</param>
    <param name="revision">distribution-gpg-keys-1.115-1</param>
    <param name="versionformat">@PARENT_TAG@</param>
    <param name="match-tag">*</param>
    <param name="changesgenerate">enable</param>
    <param name="filename">distribution-gpg-keys</param>
    <param name="versionrewrite-pattern">distribution-gpg-keys-(\d+)\.(\d+)-(\d+)</param>
    <param name="versionrewrite-replacement">\1.\2</param>
  </service>
  <service mode="manual" name="set_version"/>
  <service name="tar" mode="buildtime"/>
  <service name="recompress" mode="buildtime">
    <param name="file">*.tar</param>
    <param name="compression">xz</param>
  </service>
</services>
