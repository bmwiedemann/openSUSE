    <param name="compression">xz</param>
    <param name="file">*.tar</param>
  </service>
  <service name="set_version" mode="manual" />
  <service name="format_spec_file" mode="manual">
    <param name="specfile">rpmlint.spec</param>
  </service>
</services>
