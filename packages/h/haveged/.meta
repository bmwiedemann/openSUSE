<package name="haveged" project="openSUSE:Factory">
  <title>Feed entropy into random pool</title>
  <description>The haveged daemon feeds the linux entropy pool with random
numbers generated from hidden processor state.

For more informations see http://www.issihosts.com/haveged/</description>
  <devel project="security" package="haveged"/>
  <build>
    <enable repository="openSUSE_11.3"/>
    <enable repository="openSUSE_11.2"/>
    <enable repository="SLE_11_SP1"/>
  </build>
</package>
