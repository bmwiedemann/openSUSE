<services>
<service name="tar_scm" mode="manual">
    <param name="scm">git</param>
    <param name="url">https://github.com/hashicorp/serf.git</param>
    <param name="revision">v0.10.2</param>
    <param name="versionformat">@PARENT_TAG@</param>
    <param name="versionrewrite-pattern">[v]?([^-+a-z]+)(.*)</param>
    <param name="versionrewrite-replacement">\1</param>
    <param name="changesgenerate">disable</param>
    <param name="filename">hashicorp-serf</param>
  </service>

  <service name="extract_file" mode="manual">
    <param name="archive">*.tar.xz</param>
    <param name="files">*/LICENSE</param>
    <param name="outfilename">LICENSE</param>
  </service>

  <service name="set_version" mode="manual"/>

  <service name="recompress" mode="manual">
    <param name="file">*.tar</param>
    <param name="compression">xz</param>
  </service>

  <service name="go_modules" mode="manual">
    <param name="archive">serf-*.tar.xz</param>
  </service>
</services>
