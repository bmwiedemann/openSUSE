[Unit]
Description=Faxqueue cleaner process
Documentation=man:faxqclean(8) 
Requires=hylafax-hfaxd.service

[Timer]
OnCalendar=hourly
AccuracySec=30m
Persistent=true

[Install]
WantedBy=timers.target
