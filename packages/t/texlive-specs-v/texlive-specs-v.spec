%post -n texlive-step
mkdir -p /var/run/texlive
> /var/run/texlive/run-mktexlsr
> /var/run/texlive/run-update
echo 'addMap STEP.map' >> /var/run/texlive/run-updmap

%postun -n texlive-step
mkdir -p /var/run/texlive
> /var/run/texlive/run-mktexlsr
> /var/run/texlive/run-update
if test $1 = 0; then
    echo 'deleteMap STEP.map' >> /var/run/texlive/run-updmap
    exit 0
fi

