#!/bin/bash
   
# simple wrapper for Pegasus's cimprovagt
# which allows providers to have separate
# SELinux policy
# see README.SUSE.Security for more info
    
provcimprovagt=/usr/lib/pegasus/"$5"-cimprovagt
     
if [[ -x "$provcimprovagt" ]]
then
  "$provcimprovagt" "$@"
else
  /usr/lib/pegasus/cimprovagt "$@"
fi
