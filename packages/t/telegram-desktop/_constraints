    </memoryperjob>
  </hardware>
  <overwrite>
    <conditions>
      <arch>aarch64</arch>
      <arch>armv7l</arch>
      <arch>armv6l</arch>
    </conditions>
    <hardware>
      <physicalmemory>
        <size unit="G">5</size>
      </physicalmemory>
      <memoryperjob>
        <size unit="M">1024</size>
      </memoryperjob>
    </hardware>
  </overwrite>
</constraints>
