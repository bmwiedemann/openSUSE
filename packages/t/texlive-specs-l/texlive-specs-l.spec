> /var/run/texlive/run-update
echo 'addMap ibarra.map' >> /var/run/texlive/run-updmap

%postun -n texlive-ibarra
mkdir -p /var/run/texlive
> /var/run/texlive/run-mktexlsr
> /var/run/texlive/run-update
if test $1 = 0; then
