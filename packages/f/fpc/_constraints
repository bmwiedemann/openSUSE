  <overwrite>
    <conditions>
      <arch>ppc</arch>
      <arch>ppc64</arch>
      <arch>ppc64le</arch>
    </conditions>
    <hardware>
      <disk>
        <size unit="G">5</size>
      </disk>
    </hardware>
  </overwrite>
  <overwrite>
    <conditions>
      <arch>aarch64</arch>
    </conditions>
    <hardware>
      <memory>
        <size unit="G">4</size>
      </memory>
    </hardware>
  </overwrite>
</constraints>
