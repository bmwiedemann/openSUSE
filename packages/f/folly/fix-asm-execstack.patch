Add unconditional section markers to assembler units to avoid the library
getting an executable stack.

---
 folly/memset.S |    4 ++++
 1 file changed, 4 insertions(+)

Index: folly-2022.05.23.00/folly/memset.S
===================================================================
--- folly-2022.05.23.00.orig/folly/memset.S
+++ folly-2022.05.23.00/folly/memset.S
@@ -18,6 +18,10 @@
 
 #define LABEL(x)     .L##x
 
+#if defined(__linux__) && defined(__ELF__)
+.section .note.GNU-stack,"",@progbits
+#endif
+
 .text
 .p2align  5, 0x90
 .global __folly_memset
