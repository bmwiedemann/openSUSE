    .. code-block:: python

      # Don't use context() anymore
      file_obj = StringIO()
      consume(print(x, file=f) for f in context(file_obj) for x in
      u'123')

      # Use a with statement instead
      file_obj = StringIO()
      with file_obj as f:
          consume(print(x, file=f) for x in u'123')
