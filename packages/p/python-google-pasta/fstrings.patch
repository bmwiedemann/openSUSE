--- a/testdata/ast/fstring.in
+++ b/testdata/ast/fstring.in
@@ -34,3 +34,13 @@ f"""{
   l!r:  {m} }"""
 
 f"{(a.b).c}"
+
+foo(f"{d}"
+    "{"
+    "}")
+
+(f'e={e}'
+ ' f={f}')
+
+('g={g}'
+ f' h={h}')
