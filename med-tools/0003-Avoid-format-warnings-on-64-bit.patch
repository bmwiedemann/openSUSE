@@ -251,7 +252,11 @@
 					fprintf(stderr,"%s = %llu\n",#entier,entier) ;\
 					fflush(stderr) ;\
 				}
-# define ISCRUTE_id(entier) ISCRUTE_int(entier)
+# define ISCRUTE_id(entier)	{\
+					ICI ;\
+					fprintf(stderr,"%s = %"PRId64"\n",#entier,entier) ;\
+					fflush(stderr) ;\
+				}
 # define RSCRUTE(reel)		{\
 					ICI ;\
 					fprintf(stderr,"%s = %f\n",#reel,reel) ;\
