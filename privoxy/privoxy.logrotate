/var/lib/privoxy/log/logfile {
	compress
	dateext
	notifempty
	create 640 privoxy root
	su privoxy privoxy
	rotate 99
	size 4M
	#maxage 365
	postrotate
		/etc/init.d/privoxyd reload
	endscript
}

/var/lib/privoxy/log/jarfile {
	compress
	dateext
	notifempty
	create 640 privoxy root
	su privoxy privoxy
	rotate 99
	size 4M
	#maxage 365
	postrotate
		/etc/init.d/privoxyd reload
	endscript
}

