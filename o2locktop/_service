<services>
  <service name="tar_scm" mode="disabled">
    <param name="url">https://github.com/ganghe/o2locktop.git</param>
    <param name="scm">git</param>
    <param name="filename">o2locktop</param>
    <param name="versionformat">1.0.10+git.%ct.%h</param>
    <param name="revision">master</param>
    <param name="changesgenerate">enable</param>
  </service>

  <service name="recompress" mode="disabled">
    <param name="file">o2locktop*.tar</param>
    <param name="compression">bz2</param>
  </service>

  <service name="set_version" mode="disabled">
    <param name="basename">o2locktop</param>
  </service>
</services>

